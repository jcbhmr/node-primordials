    "tsx": "^3.12.7",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.3"
  },
  "typedocOptions": {
    "entryPoints": [
      "src/index.ts"
    ],
    "out": "docs/dist",
    "skipErrorChecking": true
