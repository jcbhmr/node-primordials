{
  "name": "@nodefill/primordials",
  "version": "1.0.0",
  "description": "ðŸ§Š The native primordials object from Node.js for anywhere",
  "type": "commonjs",
  "exports": {
    ".": "./src/index.js",
    "./*.js": "./src/*.js",
    "./internal/*": null
  },
  "scripts": {
    "build:docs": "typedoc",
    "docs:preview": "serve docs/dist",
    "lint": "prettier --ignore-path .gitignore -w .",
    "test": "node --test"
  },
  "devDependencies": {
    "@types/node": "^20.3.2",
    "prettier": "^2.8.8",
    "prettier-plugin-jsdoc": "^0.4.2",
    "prettier-plugin-packagejson": "^2.4.3",
    "serve": "^14.2.0",
    "typedoc": "^0.24.8"
  },
  "typedocOptions": {
    "entryPoints": [
      "./src/*.js"
    ],
    "out": "docs/dist",
    "skipErrorChecking": true
  }
}
